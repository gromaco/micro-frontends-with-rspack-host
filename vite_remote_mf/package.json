{
  "name": "vite_remote_mf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:adapter": "vite build --config vite.adapter.config.ts",
    "watch:adapter": "vite build --watch --config vite.adapter.config.ts",
    "serve:adapter": "npx serve dist -l 5002 --cors",
    "dev:adapter": "npx concurrently \"npm run watch:adapter\" \"npm run serve:adapter\""
  },
  "dependencies": {
    "@module-federation/enhanced": "^0.13.1",
    "@module-federation/vite": "^1.2.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
